        <StackLayout Padding="12"
                     BackgroundColor="#576ff4">
            <Button Text="Perfil" Clicked="MeusDadosClicked" BackgroundColor="#c5cefb"></Button>
            <Button Text="Agendamento" Clicked="ConsultasClicked" BackgroundColor="#c5cefb"></Button>
